{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/index.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently \"vite --config vite.renderer.config.mts\" \"vite build --config vite.preload.config.ts --outDir .vite/preload --watch\" \"node ./scripts/wait-for-preload.js && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron-forge start\"",
    "build:renderer": "vite build --config vite.renderer.config.mts",
    "start:prod": "npm run build:renderer && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prisma:generate": "prisma generate --schema=./src/main/repositories/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./src/main/repositories/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/main/repositories/prisma/schema.prisma",
    "seed:mail": "node ./scripts/seed-prisma-mail.js",
    "reset:admin": "node ./scripts/reset-admin-password.js",
    "setup:gmail": "node ./scripts/setup-gmail-oauth.js",
    "test:gmail": "node ./scripts/test-gmail-connection.js"
  },
  "keywords": [],
  "author": {
    "name": "=",
    "email": "Rachidzerga@yahoo.fr"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@prisma/client": "^6.19.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.24",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "38.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "prisma": "^6.19.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.20",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  }
}
